#include "holberton.h"
/**
* get_func - search and return the correct function
* @s: array to check
* Return: return a function (if written correctly)
**/
int (*get_func(char s))(va_list)
{
	op_t ops[] = {
		{"u", print_u},
		{"o", print_o},
		{"x", print_x},
		{"X", print_X},
		{"S", print_S},
		{NULL, NULL}
	};

	int i;

	for (i = 0; ops[i].c != NULL; i++)
	{
		if (*ops[i].c == s)
			return (ops[i].f);
	}
	return (NULL);
}
